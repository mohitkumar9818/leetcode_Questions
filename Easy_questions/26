
class Solution {
    public int removeDuplicates(int[] arr) {
       for(int i=0;i<arr.length;i++){
        for(int j=i+1;j<arr.length;j++){

            if(arr[i]==arr[j]){
                arr[j]=101;
            }else break;

        }
        }
        Arrays.sort(arr);
        int k=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]<101){
                k++;
            }
            }return k;
       
       
    }
}

